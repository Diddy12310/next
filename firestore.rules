service cloud.firestore {
  match /databases/{database}/documents {
  	// match /{document=**} {
  	// allow read, write: if false
  	// }
  	
    match /{document=**} {
      allow read, write: if request.auth.uid != null;
    }
    match /paradigm/config {
      allow read, write;
    }
    match /users/{user} {
      allow read, write;
    }
    match /colors/{color} {
      allow read, write;
    }
  }
}